<!-- Product -->
<div class="bg-light m-t-23 p-b-140 mycustomOne">
  <div class="container ">
    <div class="flex-w flex-sb-m p-b-52">
      <div class="flex-w flex-l-m filter-tope-group m-tb-10">
        <a href="/shop"><button
            class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5 how-active1"
            data-filter="*"
          >
           All products
          </button></a>
        {{#each categoryData}}
         <a href="/viewProductByCategory/{{this._id}}">
          <button
            class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5 how-active1"
            data-filter="*"
          >
            {{this.category}}
          </button>
          </a>
        {{/each}}
        
      </div>

      

      
    </div>

    <div class="row isotope-grid">
      {{#each productData}}
        <div class="col-sm-6 col-md-4 col-lg-3 p-b-35 isotope-item women">
          <!-- Block2 -->
          <div class="block2">
            <div class="block2-pic hov-img0">
              <img
                style="width: 250px;height: 230px"
                src="/product_uploads/{{this.image.[0]}}"
                alt="IMG-PRODUCT"
              />

              
            </div>

            <div class="block2-txt flex-w flex-t p-t-14">
              <div class="block2-txt-child1 flex-col-l">
                <a
                  href="/productDetails/{{this._id}}"
                  class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6"
                >
                  {{this.name}}
                </a>

                <span class="stext-105 cl3">
                  Rs.{{this.price}}
                </span>
                
              </div>

              <div class="block2-txt-child2 flex-r p-t-3">
                
                <button onclick="addToWishlist('{{this._id}}')" class="btn-addwish-b2 dis-block pos-relative js-addwish-b"><img
                    class="icon-heart1 dis-block trans-04"
                    src="/user/images/icons/icon-heart-01.png"
                    alt="ICON"
                  />
                  </button>
                  
                
              </div>
            </div>
          </div>
        </div>
      {{/each}}

    </div>

    <!-- Load more -->
    <div class="flex-c-m flex-w w-full p-t-45">
      <a
        href="#"
        class="flex-c-m stext-101 cl5 size-103 bg2 bor1 hov-btn1 p-lr-15 trans-04"
      >
        Load More
      </a>
    </div>
  </div>
</div>

<!-- Back to top -->
<div class="btn-back-to-top" id="myBtn">
  <span class="symbol-btn-back-to-top">
    <i class="zmdi zmdi-chevron-up"></i>
  </span>
</div>





























